              "                     phone = phone.replace(/\\s+/g, '');\n",
              "                     if (phone == \"\" || phone == null || isNaN(phone)) {\n",
              "                         document.getElementById(\"ctl00_ctl00_ContentPlaceHolderFooter_errno1_home\").style.display = \"inline-block\";\n",
